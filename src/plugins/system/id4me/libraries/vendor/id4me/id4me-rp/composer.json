{
    "name": "id4me/id4me-rp",
    "description": "Library for ID4me",
    "type": "library",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "ext-openssl": "*",
        "php": ">=7.0",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "ext-readline": "*",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/psr7": "1.5.*",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Id4me\\RP\\" : "src/",
            "Id4me\\Test\\" : "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "archive": {
        "exclude": ["/tests", "/examples", "/.gitignore", "/.editorconfig", "/phpunit.xml", "/docker-compose.yml", "/Dockerfile"]
    }
}
